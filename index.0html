          </span><span><span class="hljs-keyword">const</span></span><span> imageData = ctx.</span><span><span class="hljs-title function_">getImageData</span></span><span>(</span><span><span class="hljs-number">0</span></span><span>, </span><span><span class="hljs-number">0</span></span><span>, canvas.</span><span><span class="hljs-property">width</span></span><span>, canvas.</span><span><span class="hljs-property">height</span></span><span>);
          </span><span><span class="hljs-keyword">const</span></span><span> code = </span><span><span class="hljs-title function_">jsQR</span></span><span>(imageData.</span><span><span class="hljs-property">data</span></span><span>, canvas.</span><span><span class="hljs-property">width</span></span><span>, canvas.</span><span><span class="hljs-property">height</span></span><span>);
          </span><span><span class="hljs-keyword">if</span></span><span> (!code) {
