#!/bin/bash

echo "=============================="
echo "🔐 Password-Protected QR Code Generator"
echo "=============================="

# Read secret message

read -p "Enter the secret message or phrase: " secret

# Read password silently

read -s -p "Enter password to protect it: " password
echo
read -s -p "Confirm password: " password_confirm
echo

if [[ "$password" != "$password_confirm" ]]; then
&nbsp; echo "❌ Passwords do not match. Exiting."
&nbsp; exit 1
fi

# Save secret to temporary file

echo "$secret" &gt; secret.txt
# Encrypt with AES-256-CBC and PBKDF2
openssl enc -aes-256-cbc -pbkdf2 -salt -iter 100000 -in secret.txt -out secret.enc -pass pass:"$password"
# Encode to base64 so it can be put in a QR code
base64 secret.enc &gt; secret.b64
# Generate QR code
qrencode -o secret-qr.png &lt; secret.b64
