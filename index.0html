.3408L9.2168 15.2305C9.36965 15.1046 9.55583 14.9207 9.90527 14.5713L14.8926 9.58301L11.666 6.35742L6.67871 11.3447ZM16.1963 5.05371C15.3054 4.16304 13.8616 4.16305 12.9707 5.05371L12.6074 5.41602L15.833 8.64258L16.1963 8.2793C17.0869 7.38845 17.0869 5.94456 16.1963 5.05371Z"></path><path d="M4.58301 1.7832C4.72589 1.7832 4.84877 1.88437 4.87695 2.02441C4.99384 2.60873 5.22432 3.11642 5.58398 3.50391C5.94115 3.88854 6.44253 4.172 7.13281 4.28711C7.27713 4.3114 7.38267 4.43665 7.38281 4.58301C7.38281 4.7295 7.27723 4.8546 7.13281 4.87891C6.44249 4.99401 5.94116 5.27746 5.58398 5.66211C5.26908 6.00126 5.05404 6.43267 4.92676 6.92676L4.87695 7.1416C4.84891 7.28183 4.72601 7.38281 4.58301 7.38281C4.44013 7.38267 4.31709 7.28173 4.28906 7.1416C4.17212 6.55728 3.94179 6.04956 3.58203 5.66211C3.22483 5.27757 2.72347 4.99395 2.0332 4.87891C1.88897 4.85446 1.7832 4.72938 1.7832 4.58301C1.78335 4.43673 1.88902 4.3115 2.0332 4.28711C2.72366 4.17203 3.22481 3.88861 3.58203 3.50391C3.94186 3.11638 4.17214 2.60888 4.28906 2.02441L4.30371 1.97363C4.34801 1.86052 4.45804 1.78333 4.58301 1.7832Z"></path></svg>Edit</button></span></div></div></div><div class="overflow-y-auto p-4" dir="ltr"><code class="whitespace-pre! language-bash"><span><span><span class="hljs-meta">#!/bin/bash</span></span><span>

</span><span><span class="hljs-built_in">set</span></span><span> -e

</span><span><span class="hljs-comment"># === Check Dependencies ===</span></span><span>
</span><span><span class="hljs-keyword">for</span></span><span> cmd </span><span><span class="hljs-keyword">in</span></span><span> zbarimg openssl zenity; </span><span><span class="hljs-keyword">do</span></span><span>
    </span><span><span class="hljs-keyword">if</span></span><span> ! </span><span><span class="hljs-built_in">command</span></span><span> -v </span><span><span class="hljs-variable">$cmd</span></span><span> &amp;&gt;/dev/null; </span><span><span class="hljs-keyword">then</span></span><span>
        zenity --error --text=</span><span><span class="hljs-string">"<span class="hljs-variable">$cmd</span></span></span><span> is required but not installed. Exiting."
        </span><span><span class="hljs-built_in">exit</span></span><span> 1
    </span><span><span class="hljs-keyword">fi</span></span><span>
</span><span><span class="hljs-keyword">done</span></span><span>

zenity --info --title=</span><span><span class="hljs-string">"Password-Protected QR Code Reader"</span></span><span> \
    --text=</span><span><span class="hljs-string">"Welcome! This will decode and decrypt a password-protected QR code."</span></span><span>

</span><span><span class="hljs-comment"># === Select QR Code File ===</span></span><span>
qr_file=$(zenity --file-selection --title=</span><span><span class="hljs-string">"Select QR Code PNG"</span></span><span> --file-filter=</span><span><span class="hljs-string">"PNG files (png) | *.png"</span></span><span>)

</span><span><span class="hljs-keyword">if</span></span><span> [[ -z </span><span><span class="hljs-string">"<span class="hljs-variable">$qr_file</span></span></span><span>" ]]; </span><span><span class="hljs-keyword">then</span></span><span>
    zenity --error --text=</span><span><span class="hljs-string">"No file selected. Exiting."</span></span><span>
    </span><span><span class="hljs-built_in">exit</span></span><span> 1
</span><span><span class="hljs-keyword">fi</span></span><span>

</span><span><span class="hljs-comment"># === Extract base64 from QR code ===</span></span><span>
base64_data=$(zbarimg --raw </span><span><span class="hljs-string">"<span class="hljs-variable">$qr_file</span></span></span><span>" 2&gt;/dev/null | </span><span><span class="hljs-built_in">tr</span></span><span> -d </span><span><span class="hljs-string">'\n'</span></span><span>)

</span><span><span class="hljs-keyword">if</span></span><span> [[ -z </span><span><span class="hljs-string">"<span class="hljs-variable">$base64_data</span></span></span><span>" ]]; </span><span><span class="hljs-keyword">then</span></span><span>
    zenity --error --text=</span><span><span class="hljs-string">"Could not decode QR code or QR code is empty."</span></span><span>
    </span><span><span class="hljs-built_in">exit</span></span><span> 1
</span><span><span class="hljs-keyword">fi</span></span><span>

</span><span><span class="hljs-comment"># === Save base64 data to temp file ===</span></span><span>
tmp_dir=$(</span><span><span class="hljs-built_in">mktemp</span></span><span> -d)
</span><span><span class="hljs-built_in">trap</span></span><span> </span><span><span class="hljs-string">'rm -rf "$tmp_dir"'</span></span><span> EXIT
</span><span><span class="hljs-built_in">echo</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$base64_data</span></span></span><span>" &gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.b64"

</span><span><span class="hljs-comment"># === Decode base64 to encrypted file ===</span></span><span>
</span><span><span class="hljs-built_in">base64</span></span><span> -d </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.b64" &gt; </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.enc"

</span><span><span class="hljs-comment"># === Prompt for password ===</span></span><span>
password=$(zenity --password --title=</span><span><span class="hljs-string">"Enter Decryption Password"</span></span><span>)

</span><span><span class="hljs-keyword">if</span></span><span> [[ -z </span><span><span class="hljs-string">"<span class="hljs-variable">$password</span></span></span><span>" ]]; </span><span><span class="hljs-keyword">then</span></span><span>
    zenity --error --text=</span><span><span class="hljs-string">"No password entered. Exiting."</span></span><span>
    </span><span><span class="hljs-built_in">exit</span></span><span> 1
</span><span><span class="hljs-keyword">fi</span></span><span>

</span><span><span class="hljs-comment"># === Attempt to decrypt ===</span></span><span>
</span><span><span class="hljs-keyword">if</span></span><span> openssl enc -d -aes-256-cbc -pbkdf2 -iter 100000 \
    -</span><span><span class="hljs-keyword">in</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.enc" -out </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.txt" \
    -pass pass:</span><span><span class="hljs-string">"<span class="hljs-variable">$password</span></span></span><span>" 2&gt;/dev/null; </span><span><span class="hljs-keyword">then</span></span><span>

    </span><span><span class="hljs-comment"># Escape &amp;, &lt;, &gt; for zenity (Pango markup safe)</span></span><span>
    secret=$(</span><span><span class="hljs-built_in">cat</span></span><span> </span><span><span class="hljs-string">"<span class="hljs-variable">$tmp_dir</span></span></span><span>/secret.txt" | sed </span><span><span class="hljs-string">'s/&amp;/\&amp;amp;/g; s/&lt;/\&amp;lt;/g; s/&gt;/\&amp;gt;/g'</span></span><span>)
    zenity --info --title=</span><span><span class="hljs-string">"Decrypted Secret"</span></span><span> --text=</span><span><span class="hljs-string">"✅ Your secret message is:\n\n<span class="hljs-variable">$secret</span></span></span><span>"

</span><span><span class="hljs-keyword">else</span></span><span>
    zenity --error --text=</span><span><span class="hljs-string">"❌ Decryption failed. Wrong password or corrupted data."</span></span><span>
    </span><span><span class="hljs-built_in">exit</span></span><span> 1
</span><span><span class="hljs-keyword">fi</span></span><span>
</span></span></code></div></div></pre>
